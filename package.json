  "bugs": {
    "url": "https://github.com/subuta/react-ssr-gql-example/issues"
  },
  "browserslist": [
    "last 1 version",
    "> 1%"
  ],
  "homepage": "https://github.com/subuta/react-ssr-gql-example#readme",
