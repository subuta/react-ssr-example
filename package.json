{
  "name": "react-ssr-gql-example",
  "version": "1.0.0",
  "description": "Server-side-rendered react with GraphQL example.",
  "main": "index.js",
  "scripts": {
    "copy": "taskr clear copy",
    "serve": "npm run copy && node src/server/index.js",
    "servep": "NODE_ENV=production node dist/server/index.js",
    "build": "npm run copy && NODE_ENV=production webpack --silent",
    "analyze": "ANALYZE=true webpack --profile --json > stats.json",
    "show-stats": "webpack-bundle-analyzer stats.json",
    "stats": "npm run analyze && npm run show-stats",
    "browsers": "npx browserslist",
    "lint": "standard",
    "fix": "standard --fix",
    "babel": "taskr babel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/react-ssr-gql-example.git"
  },
